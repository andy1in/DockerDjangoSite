server {
    server_name onuyerinriy.beget.app www.onuyerinriy.beget.app;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/django/polinclub;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/onuyerinriy.beget.app/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/onuyerinriy.beget.app/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = www.onuyerinriy.beget.app) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = onuyerinriy.beget.app) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name onuyerinriy.beget.app www.onuyerinriy.beget.app;
    return 404; # managed by Certbot




}